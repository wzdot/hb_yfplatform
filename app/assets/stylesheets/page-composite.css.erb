body{ font-family: '微软雅黑','microsoft yahei','Apple LiGothic Medium','SimHei',Arial, Helvetica, sans-serif;padding-top:90px 0 40px 20px; background:none; }
.container{ width:100%; }
.container-fluid .row-fluid{ margin-top:20px; width:100%;}
div.container-fluid   div.span2{display:none;}
.row-fluid  .span10{ margin-left:8.5%;}
div.container  div.span10 .notice{ margin-top:20px;}
.active-scaffold-header h2{padding:0;padding-left:10px;font-size:12px; line-height:23px; color:#fff; font-weight:normal; background:#000;
font-family:inherit;
border-radius:5px 5px 0 0;
background: rgb(63,147,144);
background: -moz-linear-gradient(top,  rgba(0,109,103,1) 0%, rgba(40,136,133,1) 50%, rgba(50,141,138,1) 50%, rgba(112,172,169,1) 100%);
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,109,103,1)), color-stop(50%,rgba(40,136,133,1)), color-stop(50%,rgba(50,141,138,1)), color-stop(100%,rgba(112,172,169,1)));
background: -webkit-linear-gradient(top,  rgba(0,109,103,1) 0%,rgba(40,136,133,1) 50%,rgba(50,141,138,1) 50%,rgba(112,172,169,1) 100%);
background: -o-linear-gradient(top,  rgba(0,109,103,1) 0%,rgba(40,136,133,1) 50%,rgba(50,141,138,1) 50%,rgba(112,172,169,1) 100%);
background: -ms-linear-gradient(top,  rgba(0,109,103,1) 0%,rgba(40,136,133,1) 50%,rgba(50,141,138,1) 50%,rgba(112,172,169,1) 100%);
background: linear-gradient(top,  rgba(0,109,103,1) 0%,rgba(32,124,202,1) 50%,rgba(41,137,216,1) 50%,rgba(125,185,232,1) 100%);
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#127c79', endColorstr='#73aeab',GradientType=0 );}
.active-scaffold-header div.actions a.new, .active-scaffold-header div.actions a.show_search{
font-family:inherit;
font-size:12px;font-weight:normal;margin-top:-5px;margin-bottom:0;padding-top:3px;padding-bottom:1px;color:#fff;}
.active-scaffold a:hover, .active-scaffold div.hover, .active-scaffold td span.hover { background:#eee;}
.active-scaffold-header div.actions a.new:hover, .active-scaffold-header div.actions a.show_search:hover{ border-radius:2px;color:#000;}
.active-scaffold a:hover, .active-scaffold div.hover, .active-scaffold td span.hover { background:#eee; border-radius:2px;color:#000;}
.active-scaffold th{ text-align:center; background:none; border-top: 1px solid #CCCCCC;}
.active-scaffold th a{ display:inline;font-size:12px;background:none;
font-family:inherit;
font-weight:bold;color:#000;}
.active-scaffold th a:visited{background:none; color:#000;}
.active-scaffold th a:hover{background:none; color:#000;}
.active-scaffold th a:active{background:none; color:#000;}
.active-scaffold th.sorted{background:none; color:#000;}
.active-scaffold th.asc a, .active-scaffold th.asc a:hover, .active-scaffold th.desc a, .active-scaffold th.desc a:hover, .active-scaffold th.loading a, .active-scaffold th.loading a:hover {
background:none; color:#000;}
.active-scaffold tr.record td.sorted {background:none; color:#000;}
.active-scaffold tr.record td.sorted{border-bottom-color:#ccc;}
.active-scaffold tr.record td {font-family:inherit;font-size:12px;}
table{ width:100%; text-align:center;}
.active-scaffold tr.record { background:none;line-height:23px;}
.active-scaffold tr.record td{border-color:#ccc;}
.active-scaffold th{border-left:1px solid #ccc;background:#eee;}
.active-scaffold th.actions{border-right:1px solid #ccc;}
.active-scaffold th p{background:none; font-family:inherit;font-size:12px;
color:#000;}
.active-scaffold tr:nth-child(2n){ background:#eee;}
.active-scaffold tr:hover{ background:#f9ef9e;}
.active-scaffold .active-scaffold-footer { background:#dff0d8; border-left:1px solid #ccc; border-bottom:1px solid #ccc;}
.active-scaffold-found  .active-scaffold-records{ text-align:center; color:#000; margin-left:10px;}
.active-scaffold-footer{ border-right:1px solid #ccc; border-radius:0 0 5px 5px;}
.active-scaffold-pagination ,.active-scaffold-found{ float:left; color:#000; font-weight:normal;
font-family:inherit; font-size:12px;}
.active-scaffold-footer .active-scaffold-pagination{ float:left; margin-left:45%; color:#000;}
.active-scaffold tr.record td.actions a, .active-scaffold tr.record td.actions div {padding:0 5px 0 0; vertical-align: baseline;
font-size:12px;font-weight:normal;font-family:inherit;
}
/*as_form search*/
.active-scaffold .view{ background:#dff0d8;}
.active-scaffold form.as_form{ text-align:left;background:#EEE;border: 1px solid #CCC;border-radius: 5px 5px 5px 5px;
    margin: 15px 0;
    padding: 10px 5px 2px 20px;}
.active-scaffold a.inline-adapter-close {margin:15px 0 0 0;}
.active-scaffold a.inline-adapter-close:hover{  background-image: url( <%= asset_path('active_scaffold/close.png') %>);}
.active-scaffold .submit{background: none repeat scroll 0 0 #DFF0D8;color: #468847;font-family: inherit;
    font-size: 12px;font-weight: normal;height: 30px;line-height: 30px;margin: -10px 0 0 10px;padding: 0 20px;}
.active-scaffold input.text-input{font-size:12px; font-weight:normal; color:#000;font-family:inherit; }
form.delate .as_cancel, form.search .as_cancel {
    color: #333333;
    font-family: inherit;
    font-size: 12px;
    font-weight: normal;
    height: 30px;
    padding: 5px 20px;
    text-decoration: underline;
}
/*create form*/
.active-scaffold h4{color:#fff; padding:0 0 0 10px; line-height:23px; font-weight:normal; font-size:12px; margin:-10px -5px 0 -20px;
font-family:inherit;
letter-spacing:1px;
border-radius:5px 5px 0 0;
background: rgb(63,147,144);
background: -moz-linear-gradient(top,  rgba(0,109,103,1) 0%, rgba(40,136,133,1) 50%, rgba(50,141,138,1) 50%, rgba(112,172,169,1) 100%);
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,109,103,1)), color-stop(50%,rgba(40,136,133,1)), color-stop(50%,rgba(50,141,138,1)), color-stop(100%,rgba(112,172,169,1)));
background: -webkit-linear-gradient(top,  rgba(0,109,103,1) 0%,rgba(40,136,133,1) 50%,rgba(50,141,138,1) 50%,rgba(112,172,169,1) 100%);
background: -o-linear-gradient(top,  rgba(0,109,103,1) 0%,rgba(40,136,133,1) 50%,rgba(50,141,138,1) 50%,rgba(112,172,169,1) 100%);
background: -ms-linear-gradient(top,  rgba(0,109,103,1) 0%,rgba(40,136,133,1) 50%,rgba(50,141,138,1) 50%,rgba(112,172,169,1) 100%);
background: linear-gradient(top,  rgba(0,109,103,1) 0%,rgba(32,124,202,1) 50%,rgba(41,137,216,1) 50%,rgba(125,185,232,1) 100%);
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#127c79', endColorstr='#73aeab',GradientType=0 );}
.ui-datepicker select.ui-datepicker-month, .ui-datepicker select.ui-datepicker-year {width:40%; height:20px; margin:-3px 5px 0 0; padding:0;line-height:20px; font-size:12px;}
.show-view h4{ margin:15px 0 0 5px;}
.active-scaffold p.form-footer{ padding-bottom:20px;}
.active-scaffold p.form-footer a { -moz-transition: border 0.2s linear 0s, box-shadow 0.2s linear 0s;
    background: none repeat scroll 0 0 #DFF0D8;
    border: 1px solid #CCCCCC;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    color: #468847;
    font-family: inherit;
    font-size: 12px;
    margin: 10px 0 20px 20px;
    padding: 5px 15px;
    text-align: center;
    width: 64px;}
.active-scaffold .show-view dt,.active-scaffold .show-view dd{font-family:inherit;font-size:12px; color:#000;line-height:25px;}	
@media screen and (max-width:1280px){
.row-fluid .span10{width:1240px;margin-left:0;}
}
