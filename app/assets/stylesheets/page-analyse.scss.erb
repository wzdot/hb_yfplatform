/*container*/
.container-fluid { 
  zoom: 1; 
  position: relative;
  zoom: 1;
  padding:0;
  margin:0;
  zoom: 1;
  font-family:'微软雅黑','microsoft yahei','Apple LiGothic Medium','SimHei',Arial, Helvetica, sans-serif;
  .row-fluid {
    margin:10px auto;
    width:1200px;
    padding:0;
    &:before,&:after {
      display: table;
      content: ""; 
    }
    &:after {
      clear: both; 
    }
    .span2{
      float:left; 
      width:190px;
      height:400px;
      margin:0 28px 40px 0;
      margin:0 28px 45px 0\9;
      *margin:0 28px 40px 0;
      padding:0;
      /*ztree style*/
      ul.ztree {
        font-family:'微软雅黑','microsoft yahei','Apple LiGothic Medium','SimHei',Arial, Helvetica, sans-serif;
        width:178px;
        height:402px;
        height:408px\9;
        *height:403px;
        _height:408px;
        border:1px solid #ccc;
        border-top: none;
        margin: 0 0 10px 0;
        margin: 0 0 15px 0\9;
        *margin: 0 0 10px 0;
        position: relative;
        li a.curSelectedNode {
          background-color: #ffe45c;
        }
      }
    }
    .span10{
      float: left;
      position: relative;
      left: 0;
      top: 0;
      z-index: 5;
      width: 980px;
      margin: 0;
      padding: 0;
      font-family: '微软雅黑','microsoft yahei','Apple LiGothic Medium','SimHei',Arial, Helvetica, sans-serif;
      font-size: 12px;    
      .query_items {
        margin: 0;
        *margin: 0 0 20px 0;
        padding: 0;
        border:1px solid #ccc;
        border-radius: 5px;
        position: relative;
        .form_part {
          float: left;
          padding: 0 0 10px 0;
          form {
            padding: 10px 0 0 10px;
            margin:0;           
            label, input, select, div, span {
              display:inline;
              font-family: inherit;
              font-size: inherit;
              vertical-align: middle;
              padding: 0;
              margin: 0;
              cursor: pointer;
            }
            select {
              font-family: '微软雅黑','microsoft yahei','Apple LiGothic Medium','SimHei',Arial, Helvetica, sans-serif;
              font-size: 12px;
              width: 80px;
              vertical-align: baseline;
            }
            input.hasDatepicker {
              width: 75px;
            }
            div.query_defect {
              margin: 0 8px 0 0;
              input {
                margin: 0 2px 0 0;
              }
            }
            div.total_number {
              cursor: default;
              span {
                cursor: default; 
              }
              em {
                font-style: normal;
                cursor: default;
                vertical-align: middle;
              }
            }
          }
        }
        .btn_part {
          float: right;
          position: relative;
          right: 5px;
          top: 0;
          z-index: 99;
          .query_reset_btn, .analyse_btn, .slider_report_btn {
            float: left;
            button {
              font-family: '微软雅黑','microsoft yahei','Apple LiGothic Medium','SimHei',Arial, Helvetica, sans-serif;
              font-size: 12px;
              display: block;
              width: 95px;
              height: 23px;
              line-height: 15px;
              text-align: center;
              margin: 7px 5px 0 0;
              padding: 0;
              cursor: pointer;
              border-radius: 3px;
              border: 1px solid #ccc;
              background-color: #f5f5f5;
              background-image: -moz-linear-gradient(top, #ffffff, #e6e6e6);
              background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#e6e6e6));
              background-image: -webkit-linear-gradient(top, #ffffff, #e6e6e6);
              background-image: -o-linear-gradient(top, #ffffff, #e6e6e6);
              background-image: linear-gradient(to bottom, #ffffff, #e6e6e6);
              background-repeat: repeat-x;
              &:hover {
                background: #fff;
              }
            }          
          }
          .analyse_btn {
            display: none;
          }
          .report_part {
            height: 35px\9;
            width: 90px\9;
            .choose_templet {
              position: absolute;
              right: -15px;
              top: 60px;
              background: #fff;
              z-index: 100;
              width: 120px;              
              border-bottom: none;
              cursor: pointer;
              display: none;
              img {
                display: block;
                position: relative;
                left: 50px; 
                z-index: 150;
                top: 1px;
                height: 11px;
                width: 18px;
              }
              ul {  
                position: relative;
                z-index: 101;
                border-radius: 5px;
                border: 1px solid #aaa;           
                li {
                  font-family: inherit;
                  font-size: inherit;
                  text-align: center;
                  padding: 5px;
                  border-bottom: 1px solid #aaa; 
                } 
                li.on_item {
                  border-radius: 5px;
                }
                li.first_item {
                  border-radius: 5px 5px 0 0;
                } 
                li.last_item {
                  border-radius: 0 0 5px 5px;
                } 
              }
            }
          }
          
        }
        
      }
      /*jqGrid*/
      .query_results {
        border-right: 1px solid #ccc;
        border-radius: 5px;
        width: 980px;
        overflow: auto;
        margin-top: 10px;
        *margin-top: -10px;
        position: relative;
        *z-index: -50; 
        .ui-jqgrid{
          font-family:'微软雅黑','microsoft yahei','Apple LiGothic Medium','SimHei',Arial, Helvetica, sans-serif;
          font-size:12px;
          color: #000;
          background: #fff;
          zoom: 1;
        } 
        .ui-jqgrid-titlebar{
          border-bottom: none;
          height: 25px;
          line-height: 25px;
          .ui-jqgrid-title {
            padding-left: 10px;
            font-weight: normal;
          }
        } 
        .ui-jqgrid-labels,.ui-jqgrid-hbox, .ui-jqgrid-hdiv {
          line-height: 27px;
          height: 27px;
          padding: 0;
        }  
        .ui-jqgrid-sortable{
          color: #000;
        }  
        .ui-jqgrid-htable th div {
          *overflow: hidden;
          *padding-bottom: 5px;
          *white-space:normal !important;           
        }
        .ui-jqgrid .ui-pg-input {
          display: inline-block;
          width: 40px;
        }
        .ui-jqgrid .ui-pg-table {
          zoom: 1;
          *width: 120%;
        }
      }
    }
  }
  .span12 {
    font-family:'微软雅黑','microsoft yahei','Apple LiGothic Medium','SimHei',Arial, Helvetica, sans-serif;
