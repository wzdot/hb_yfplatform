.picturebox {
  position: fixed;
  *position: absolute;
  top: 15%;
  *top: 30%;
  left: 30%;
  z-index: 1000;
  img{
    height: 480px;
    width: 640px;
  }
  .closeimg{
    position: absolute;
    top: 0;
    right: 0;
    cursor: pointer;
    width: 16px;
    height: 17px;
  }
}

#layer {
  position: absolute;
  width: 100%;
  height: 1050px;
  background: #333;
  z-index: 200;
  display: none;
  margin-top: -200px;
  filter:alpha(opacity=50);
  -moz-opacity:0.5;  
  -khtml-opacity: 0.5;
   opacity: 0.5;
}

