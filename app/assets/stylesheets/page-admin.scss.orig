body { 
  font-family: '微软雅黑','microsoft yahei','Apple LiGothic Medium','SimHei',Arial, Helvetica, sans-serif;
  background: none;
  padding: 80px 0 40px 0;}
.dropdown-menu li { list-style: none; }	   
.span2 { margin-top: 5px;
  height: 360px;
  .well { border-radius: 5px;
    background-color: #fff;  }}	

.span6 { padding-left : 20px;
  .alert {
    background-color: #DFF0D8;
    border-color: #D6E9C6;
    border-radius: 5px 5px 5px 5px;
    color: #468847;
    line-height: 27px;
    margin-bottom: 5px;
    padding-left: 10px;
	p { font-size: 13px;}
  }
  span { 
    font-size: 13px;
	a { font-size: 13px;}
  }
}	
    
h2 {
  font-family:inherit;
  font-weight: normal;
  padding: 0; 
  color: #fff;
  font-size:14px; 
  line-height:27px;
  text-indent: 20px;  
  small { 
    color: #dff0d8;
	font-size: 12px;
	margin-left: 10px;
	line-height: 20px;
  }
}  
h2.bg_gradient{  
  border-radius: 4px;
  a {
    line-height: 20px;
    float: right;
    text-align: left;
	padding:2px 15px 3px 0; 
    display: block;
	font-size: 12px;
	margin:0 20px 0 0;}
  }

.nav-pills {
  li { 
    font-size: 13px;
    list-style: none;
    margin-right: 15px;
    a { background-color: #eee;
	  border: #ccc 1px solid;
	  color: #666;
	  &:hover {
	    background-color: #ddd;
   	    color: #000;
	  }
	}
  }
  li.active{
    a { background:#DFF0D8;
      color: #468847;
      border-color: #83c584;	  
	  &:hover { background:#DFF0D8;
        color: #468847;
	  } 
    }
  }
}
table.admin-table { 
  border-collapse: separate;
  border-spacing: 0;
}
table.admin-table thead:first-child tr:first-child th:first-child, table.admin-table tbody:first-child tr:first-child td:first-child {
  border-top-left-radius: 4px;
}
table.admin-table thead:first-child tr:first-child th:last-child, table.admin-table tbody:first-child tr:first-child td:last-child {
  border-top-right-radius: 4px;
}
table.admin-table thead:last-child tr:last-child th:first-child, table.admin-table tbody:last-child tr:last-child td:first-child {
  border-radius: 0 0 0 4px;
}
table.admin-table thead:last-child tr:last-child th:last-child, table.admin-table tbody:last-child tr:last-child td:last-child {
  border-bottom-right-radius: 4px;
}
table.admin-table{
  width: 100%;
  border-radius: 4px;
  border: 1px solid #ccc;
  border-right: none;
  border-bottom: none;
  thead tr { background: #eee;}
  td,th {
    font-size: 13px;
    text-align: center;
    line-height: 33px;
	border-right: 1px solid #ccc;
	border-bottom: 1px solid #ccc;
	a {
	  &:hover { background: none;}
	}
    .btn { font-size: 13px;
	  vertical-align: baseline;}	
  }
} 
table.table-bordered {
  border-radius: 4px;
  width: 100%;
  border-bottom: none;
  tr {
    td {
	  font: { 
	    weight: normal;
	    size: 13px;}
      text-align: center;
      line-height: 30px;
      border-bottom: 1px solid #ddd; 
    }
	&:hover {
	  background: #f9ef9e;
	}
	&:nth-child(2n){
	  background: #eee;
	}
  }  
}
 
div.clearfix {
  display: block; 
  label { 
    font-size: 13px;
    display: inline-block;
	width: 180px; }
  .input { 
    display: inline-block; 
	.help-inline {
	  font-family: inherit;
	  color: #C09853; } 
	}
  .field_with_errors {
	display: inline-block;
	width: 200px;
	input { margin-left: -20px;}
    label { 
      font-size: 13px;
      display: inline-block;}	
	}
  }   
  
#error_explanation {
  background-color: #FCF8E3;
  border: 1px solid #FBEED5;
  border-radius: 4px 4px 4px 4px;
  color: #C09853;
  margin-bottom: 18px;
  padding: 8px 35px 0 14px;
  text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
  ul li { line-height: 23px;}
  }  

.actions { 
  margin-left: 210px;
  .btn{
    height: 30px;
    line-height: 30px;   
    font-family: inherit;
    font-size: 13px;
    font-weight: normal;
    padding: 0 10px;	
  }
  input.btn { 
    padding: 0 20px;
    margin-right: 20px;
    background: none repeat scroll 0 0 #DFF0D8;
    color: #468847; }	
  } 
  

@media screen and (max-width:1280px) {
  .container-fluid .row-fluid {
    width: 1280px;
    overflow: auto; }
  .row-fluid {
    .span2 {
      width: 180px; }
    .span10 {
      width: 1020px; } } }  
	  

